<ul id="users">
	<% @all_users.each do |user| %>
		<li class="user">
			<div class="f_name col"><%=user.first_name%></div>
			<div class="l_name col"><%=user.last_name%></div>
			<div class="email col"><%=user.email%></div>
			<div class="username col"><%=user.username%></div>
		</li>
	<% end %>
</ul>
